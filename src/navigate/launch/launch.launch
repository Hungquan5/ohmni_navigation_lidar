<launch>
    <node pkg="tf" type="static_transform_publisher" name="base_to_laser_broadcast"
        args="0 0 0 0 0 0 base_link laser 100"/>

 <!--- Run the map server --> 
    <node name="map_server" pkg="map_server" type="map_server" args="$(find navigate)/map/aiclub.yaml"/>

 <!--- Run AMCL --> 
    <include file="$(find amcl)/examples/amcl_omni.launch" />

   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find navigate)/config/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find navigate)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find navigate)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find navigate)/config/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find navigate)/config/base_local_planner_params.yaml" command="load" />
 </node>

</launch> 
